<?xml version="1.0" encoding="UTF-8"?>

<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	 xmlns="http://www.tibco.com/schemas/EBM_Channel_BILLER_PAYMENT/SharedResources/Schemas/Schema.xsd"
	 targetNamespace="http://www.tibco.com/schemas/EBM_Channel_BILLER_PAYMENT/SharedResources/Schemas/Schema.xsd"
	 elementFormDefault="qualified"
	 attributeFormDefault="unqualified">
	<xs:element name="BILLER_PAYMENT">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="CUSTOMER_ID" type="xs:string"/>
				<xs:element name="ACCOUNT_NO" type="xs:string"/>
				<xs:element name="PRODUCT_CODE" type="xs:string"/>
				<xs:element name="PRODUCT_NAME" type="xs:string"/>
				<xs:element name="TRAN_DATETIME" type="xs:string"/>
				<xs:element name="AMOUNT" type="xs:string"/>
				<xs:element name="STATUS" type="xs:string"/>
				<xs:element name="UNIQUE_ID" type="xs:string" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>